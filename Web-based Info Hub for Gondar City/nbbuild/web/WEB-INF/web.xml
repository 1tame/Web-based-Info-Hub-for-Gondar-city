<?xml version="1.0" encoding="UTF-8"?>

<element>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
                             http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

   
    <servlet>
        <servlet-name>RegistrationServlet</servlet-name>
        <servlet-class>info_hub_management.RegistrationServlet</servlet-class>
    </servlet>


    <servlet-mapping>
        <servlet-name>RegistrationServlet</servlet-name>
        <url-pattern>/RegisterationServlet</url-pattern>
    </servlet-mapping>

 <servlet>
        <servlet-name>AdminLoginServlet</servlet-name>
        <servlet-class>info_hub_management.AdminLoginServlet</servlet-class>
    </servlet>

    <!-- Servlet mapping for AdminLoginServlet -->
    <servlet-mapping>
        <servlet-name>AdminLoginServlet</servlet-name>
        <url-pattern>/AdminLoginServlet</url-pattern>
    </servlet-mapping>
    
    
    
    <servlet>
        <servlet-name>EventFestivalInfoServlet</servlet-name>
        <servlet-class>info_hub_management.EventFestivalInfoServlet</servlet-class>
    </servlet>

    <!-- Servlet mapping for EventFestivalInfoServlet -->
    <servlet-mapping>
        <servlet-name>EventFestivalInfoServlet</servlet-name>
        <url-pattern>/submitEventFestivalInfo</url-pattern>
    </servlet-mapping>

    <!-- Servlet for inserting Health Sector Information -->
    <servlet>
        <servlet-name>HealthSectorInfoServlet</servlet-name>
        <servlet-class>info_hub_management.HealthSectorInfoServlet</servlet-class>
    </servlet>

    <!-- Servlet mapping for HealthSectorInfoServlet -->
    <servlet-mapping>
        <servlet-name>HealthSectorInfoServlet</servlet-name>
        <url-pattern>/HealthSectorInfoServlet</url-pattern>
    </servlet-mapping>


<servlet>
        <servlet-name>SubmitSecurityInfoServlet</servlet-name>
        <servlet-class>info_hub_management.SubmitSecurityInfoServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>SubmitSecurityInfoServlet</servlet-name>
        <url-pattern>/submitSecurityInfo</url-pattern>
    </servlet-mapping>
    
    
    <servlet>
        <servlet-name>SubmitTransportationInfoServlet</servlet-name>
        <servlet-class>info_hub_management.SubmitTransportationInfoServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>SubmitTransportationInfoServlet</servlet-name>
        <url-pattern>/submitTransportationInfo</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>SubmitTourismInfoServlet</servlet-name>
        <servlet-class>info_hub_management.SubmitTourismInfoServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>SubmitTourismInfoServlet</servlet-name>
        <url-pattern>/submitTourismInfo</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>SubmitWaterSupplyInfoServlet</servlet-name>
        <servlet-class>info_hub_management.SubmitWaterSupplyInfoServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>SubmitWaterSupplyInfoServlet</servlet-name>
        <url-pattern>/submitWaterSupplyInfo</url-pattern>
    </servlet-mapping>
    


</web-app>
</element>